#!pako

struct A {
  B int
  C float64
  T string
}

fn |A| GetT() {
  return self.T
}

fn |A| AddToB(k) {
  self.B += k
}

a = make(A)
a.B = 20
a.C = 20.124451
a.T = "Hello!!!!"

a.AddToB(1)

println(a.GetT())

println(a)

b = make(B)
b.C = make(A)
b.C.T = "Hello3"
println(b.C.T)
println(b.C.GetT())

struct B {
  C A
}
