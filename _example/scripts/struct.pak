#!pako

struct A {
  B int
  C float64
  T string
}

fn |A| GetT() {
  return self.T
}

fn |A| AddToB(k) {
  self.B += k
}

a = make(A)
a.B = 20
a.C = 20.124451
a.T = "Hello!!!!"

a.AddToB(1)
a.AddToB(19)

println(a.GetT())
println(a)

b = make(B)
b.C.T = "Hello2 !!!"
println(b.C.GetT())

struct B {
  C A
}

// constructor
fn |B| New() {
  self.C = make(A)
}
